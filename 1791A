#include <stdio.h>
#include <string.h>

int main() {
    int t;
    
    scanf("%d", &t);

    while (t--) 
    {
        char str[] = "codeforces";  
        char target;
    scanf(" %c", &target);
    int found = 0;
        for (int i = 0; i < strlen(str); i++) {
            if (str[i] == target) {
                found = 1;  
                break;      
            }
        }

        
        if (found) {
            printf("YES\n", target);
        } else {
            printf("NO\n", target);
        }
    }

    return 0;
}
